{
  "name": "@onmiddleground/core-dynamo",
  "version": "1.0.13",
  "description": "Useful Dynamo Tools for development and testing",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "clean": "node ./node_modules/rimraf/bin.js dist",
    "build": "npm run clean && node ./node_modules/typescript/bin/tsc",
    "watch": "npm run clean && node ./node_modules/typescript/bin/tsc --watch",
    "test": "mocha -r ts-node/register src/**/*-spec.ts",
    "publish": "npm run build && npm version patch && npm publish"
  },
  "author": "Gary Waddell",
  "license": "ISC",
  "dependencies": {
    "@onmiddleground/dynamo-tools": "^1.0.4",
    "class-validator": "^0.14.0",
    "dayjs": "^1.10.5"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.0",
    "@types/sinon": "^10.0.13",
    "aws-sdk": "^2.1359.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "mocha": "^10.2.0",
    "sinon": "^15.0.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.8.2",
    "tsc-watch": "^6.0.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "@types/validator": "^13.7.10",
    "ksuid": "^3.0.0",
    "rimraf": "^3.0.2"
  },
  "repository": {
    "directory": "dist/"
  }
}
