{
  "name": "@onmiddleground/core-dynamo",
  "version": "1.0.9",
  "description": "Useful Dynamo Tools for development and testing",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "clean": "node ./node_modules/rimraf/bin.js dist",
    "build": "npm run clean && node ./node_modules/typescript/bin/tsc",
    "watch": "npm run clean && node ./node_modules/typescript/bin/tsc --watch",
    "test": "mocha -r ts-node/register src/**/*-spec.ts",
    "release:publish": "npm run build && npm version patch --force && npm publish",
    "local:publish": "npm run build && npm unpublish --force && npm publish"
  },
  "author": "Gary Waddell",
  "license": "ISC",
  "dependencies": {
    "@onmiddleground/dynamo-tools": "^1.0.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.10.5"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^14.18.34",
    "@types/sinon": "^10.0.13",
    "@types/validator": "^13.7.10",
    "aws-sdk": "^2.1270.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "ksuid": "^3.0.0",
    "mocha": "^10.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^15.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "tslib": "^2.4.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3"
  },
  "repository": {
    "directory": "dist/"
  }
}
